[Enable]
alloc(Current_Level,128)
alloc(lvl,4)
registersymbol(lvl)

Current_Level:
mov ecx,[ecx+00001640]
mov [lvl],ecx
jmp 01B9BF67+6

01B9BF67: // 8B 89 ? ? ? ? 41 51 E8 ? ? ? ? 83 C4 10
jmp Current_Level
db 90

[Disable]
01B9BF67:
mov ecx,[ecx+00001640]

dealloc(Current_Level)
dealloc(lvl)
unregistersymbol(lvl)
