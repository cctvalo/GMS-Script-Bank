//  81 ? 53 24 14 00 75 08 [function start and breakpoint]
//  FF 75 0C E8 ? ? 08 00 5E 5D C2 08 00 FF 75 0C E8 ? ? 08 00 [Second Result] [Follow Call]

[enable]
